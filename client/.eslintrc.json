{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-prototype-builtins": [
      "warn"
    ],
    "react/display-name": [
      0
    ],
    "no-multiple-empty-lines": [
      1,
      {
        "max": 1
      }
    ],
    "react/prop-types": 0,
    "indent": [
      0,
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "warn",
      "single"
    ],
    "semi": [
      "warn",
      "always"
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "log",
          "warn",
          "error"
        ]
      }
    ],
    "comma-dangle": [
      "warn",
      "always-multiline"
    ],
    "no-unused-vars": 0
  }
}